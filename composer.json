{
    "name": "ricorocks-digital-agency/morpher",
    "description": "A Laravel package for managing data changes during database migrations.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.13",
        "pestphp/pest-plugin-expectations": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RicorocksDigitalAgency\\Morpher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicorocksDigitalAgency\\Morpher\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "luke",
            "email": "lukeraymonddowning@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "RicorocksDigitalAgency\\Morpher\\Providers\\MorpherServiceProvider"
            ],
            "aliases": {
                "Morpher": "RicorocksDigitalAgency\\Morpher\\Facades\\Morpher"
            }
        }
    }
}
